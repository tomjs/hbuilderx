{
  "name": "@tomjs/hbuilderx",
  "type": "commonjs",
  "version": "0.0.0",
  "description": "为 HBuilderX 的插件开发提供所需的types、API，方便结合 typescript、tsdown、vite等现代化工具使用",
  "author": {
    "name": "Tom Gao",
    "email": "tom@tomgao.cc"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomjs/hbuilderx.git",
    "directory": "packages/hbuilderx"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./types": "./hbuilderx.d.ts",
    "./client": "./client.d.ts"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "client.d.ts",
    "hbuilderx.d.ts"
  ],
  "engines": {
    "node": ">=16.17.0"
  },
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "lint": "eslint --fix",
    "prepublishOnly": "pnpm build"
  }
}
